{
  "name": "final-fantasy-x",
  "version": "1.0.0",
  "description": "Final Fantasy X Any% Speedrun Notes.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate-trackables": "ts-node --project tsconfig.scripts.json ./scripts/generate-trackables.ts",
    "generate-flag-map": "ts-node --project tsconfig.scripts.json ./scripts/generate-flag-map.ts",
    "watch-types": "onchange \"src/types.ts\" -- npm run generate-trackables",
    "watch-data-for-flag-map": "onchange \"public/data/**/*.json\" -- npm run generate-flag-map",
    "dev": "concurrently \"vite\" \"npm run watch-types\" \"npm run watch-data-for-flag-map\"",
    "predev": "npm run generate-trackables && npm run generate-flag-map",
    "build": "npm run generate-trackables && npm run generate-flag-map && vite build",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.14",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "fs-extra": "^11.3.0",
    "onchange": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "glob": "^11.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
